{
  "name": "nukefromorbit/nuke-php",
  "description": "Nuke PHP library",
  "keywords": [
    "nuke",
    "nuke from orbit"
  ],
  "homepage": "https://nuke.app/",
  "authors": [
    {
      "name": "Alexandru Dobrescu",
      "email": "alex@nuke.app"
    }
  ],
  "require": {
    "php": ">=8.1.29",
    "ext-mbstring": "*",
    "ext-openssl": "*"
  },
  "autoload": {
    "psr-4": {
      "Nuke\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nuke\\": "tests/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.64",
    "phpunit/phpunit": "^11.4",
    "squizlabs/php_codesniffer": "^3.10",
    "php-mock/php-mock": "^2.5"
  }
}
